<h1>Data Item List</h1>

<table class="table table-hover">
   <tr class="info">
	    <th>Title</th>
	    <th>Type</th>
	    <th>Is_active</th>
   </tr>

   <% @data_items.each do |d| %>

   <tr class="active">
      
      <td><%= link_to d.title, artist_data_item_content_items_path(@artist.id,d.id) %></td>
      <td><%= d.type %></td>
      <td><%= d.is_active %></td>
      
     
    </tr>
  <% end %>
</table>