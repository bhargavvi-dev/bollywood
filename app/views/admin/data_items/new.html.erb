<h1>New Data Items</h1>

<div class="container">
  <div class="show-profile-box">
    <div class="row">
      <div class="col-lg-12">
        <div class="panel with-nav-tabs panel-primary">
          <ul class="nav nav-tabs" id="profileTabs">
            <li class="active">
              <a data-toggle="tab" href="#public-profile">
                <i class="fa fa-eye"></i>
                News
              </a>
            </li>
            <li>
              <a data-toggle="tab" href="#private-profile">
                <i class="fa fa-lock"></i>
                Events
				</a>
            </li>
            <li>
              <a data-toggle="tab" href="#allowed-private-profile">
                <i class="fa fa-key"></i>
                 Photo
              </a>
            </li>
          <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade in active" id="public-profile">
              <%= render :partial => "news" %>
            </div>
            <div class="tab-pane fade" id="private-profile">
              <%= render :partial => "events" %>
            </div>
            <div class="tab-pane fade" id="allowed-private-profile">
              <%= render :partial => "photo" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
 
<%= link_to t('menu.back'), admin_data_items_path, :class => "btn btn-primary" %>

