<h1>Artist List</h1>
<table class="table table-hover" id="table_id">
<thead>
  <tr>
    <th>Real Name</th>
    <th>Nick Name</th>
    <th>Address</th>
    <th>DOB</th>
    <th>Hometown</th>
    <th>Religion</th>
    <th>Nationality</th>
  </tr>
  </thead>
 <tbody>
  <% @artists.each do |artist| %>
    <tr>
      <td><%= link_to artist.real_name, artist_data_items_path(artist.id) %></td>
      <td><%= artist.nick_name %></td>
      <td><%= artist.address %></td>
      <td><%= artist.dob %></td>
      <td><%= artist.hometown %></td>
      <td><%= artist.religion %></td>
      <td><%= artist.nationality %></td>
    </tr>
  <% end %>
  </tbody>
</table>
