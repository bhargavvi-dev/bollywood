<h1>Artist List</h1>
<table class="table table-hover">
  <tr>
    <th>Real Name</th>
    <th>Nick Name</th>
    <th>Address</th>
    <th>DOB</th>
    <th>Hometown</th>
    <th>Religion</th>
    <th>Nationality</th>
  </tr>
 
  <% @artists.each do |artist| %>
    <tr>
      <td><%= artist.real_name %></td>
      <td><%= artist.nick_name %></td>
      <td><%= artist.address %></td>
      <td><%= artist.dob %></td>
      <td><%= artist.hometown %></td>
      <td><%= artist.religion %></td>
      <td><%= artist.nationality %></td>
     <!--  <td><%= link_to image_tag("/show.jpg"), artist_path(artist) %></td>
      <td><%= link_to image_tag("/edit.png"), edit_artist_path(artist) %></td>
      <td><%= link_to image_tag("/delete.png"), artist_path(artist),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td> -->
    </tr>
  <% end %>
</table>
<%= link_to 'New artist', new_artist_path %>